<div id="modal8" class="modal">
    <div class="modal-content">
        <h4>ADD TARLAC BROADCAST</h4>
        <div class="row">
            <div class="card col s12" style="border-left:solid;border-left-color:blue;">
                <div class="progress">
                    <div class="indeterminate" hidden></div>
                </div>
                <div class="card-content">
                    <div class="row">
                        <div class="card-image col s6" style="width:300px">
                            <img id="image-change" class="materialboxed" src="~/assets/imgs/no-image-available.png">
                            <a class="btn-floating halfway-fab waves-effect waves-light red" id="browse_image_for_current"><i class="material-icons">add</i></a>
                            <input type="file" class="hide-file-button" id="current_images" multiple hidden onchange="showimagepreview(this)" />
                        </div>
                        <div class="col s4">
                            <div class="row">
                                <div class="input-field">
                                    <input placeholder="Placeholder" id="safety-date" type="date" class="validate">
                                    <label for="safety-date">Enter Expiry of Broadcast</label>
                                </div>
                            </div>
                            <div class="row">
                                <div class="input-field">
                                    <input id="broadcast-title" type="text" class="validate">
                                    <label for="broadcast-title">Broadcast Title</label>
                                </div>
                            </div>
                        </div>

                    </div>

                    <div class="input-field col s12">
                        <textarea placeholder="Placeholder" id="safety-message" type="date" class="validate materialize-textarea"></textarea>
                        <label for="safety-message">Message</label>
                    </div>
                    <div class="input-field col s12">
                        <a class="waves-effect waves-light btn" id="submit-safety-calendar-message" style="float:right">SUBMIT</a>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>

